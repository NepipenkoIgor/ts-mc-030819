{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "variable-name": [false],
        "quotemark": [
            true,
            "single"
        ],
        "no-console": [
            true,
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "max-classes-per-file": [
            true,
            100
        ],
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
